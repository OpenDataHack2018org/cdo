@BeginModule
@NewPage
@Name      = Timstat
@Title     = Statistical values over all time steps
@Section   = Statistical values
@Class     = Statistic
@Arguments = ifile ofile
@Operators = timmin timmax timsum timmean timavg timvar timstd

@BeginDescription
This module computes statistical  values over all time steps in @file{ifile}.
Depending on the actual operator the minimum, maximum, sum, average, variance
or standard deviation of all time steps read from @file{ifile} is written to @file{ofile}.
The date information of a time step in @file{ofile} is the date of the last 
contributing time step in @file{ifile}.
@EndDescription
@EndModule


@BeginOperator_timmin
@Title     = Time minimum

@BeginDescription
@IfMan
o(1,x) = min{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\bf min}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timmax
@Title     = Time maximum

@BeginDescription
@IfMan
o(1,x) = max{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\bf max}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timsum
@Title     = Time sum

@BeginDescription
@IfMan
o(1,x) = sum{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\bf sum}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timmean
@Title     = Time mean

@BeginDescription
@IfMan
o(1,x) = mean{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\bf mean}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timavg
@Title     = Time average

@BeginDescription
@IfMan
o(1,x) = avg{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\bf avg}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timvar
@Title     = Time variance

@BeginDescription
@IfMan
o(1,x) = var{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\bf var}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timstd
@Title     = Time standard deviation

@BeginDescription
@IfMan
o(1,x) = std{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\bf std}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginExample
To compute the mean over all input time steps use:
@BeginVerbatim
   cdo timmean ifile ofile
@EndVerbatim
@EndExample
