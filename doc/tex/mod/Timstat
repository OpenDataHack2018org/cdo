@BeginModule
@NewPage
@Name      = Timstat
@Title     = Statistical values over all timesteps
@Section   = Statistical values
@Class     = Statistic
@Arguments = ifile ofile
@Operators = timmin timmax timsum timmean timavg timstd timstd1 timvar timvar1

@BeginDescription
This module computes statistical  values over all timesteps in @file{ifile}. Depending on 
the chosen operator the minimum, maximum, sum, average, variance or standard deviation of 
all timesteps read from @file{ifile} is written to @file{ofile}.
The time of @file{ofile} is determined by the time in the middle of all contributing timesteps of @file{ifile}.
@EndDescription
@EndModule


@BeginOperator_timmin
@Title     = Time minimum

@BeginDescription
@IfMan
o(1,x) = min{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{min}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timmax
@Title     = Time maximum

@BeginDescription
@IfMan
o(1,x) = max{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{max}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timsum
@Title     = Time sum

@BeginDescription
@IfMan
o(1,x) = sum{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{sum}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timmean
@Title     = Time mean

@BeginDescription
@IfMan
o(1,x) = mean{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{mean}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timavg
@Title     = Time average

@BeginDescription
@IfMan
o(1,x) = avg{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{avg}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timvar
@Title     = Time variance

@BeginDescription
Normalize by n. 

@IfMan
o(1,x) = var{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{var}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timvar1
@Title     = Time variance (n-1)

@BeginDescription
Normalize by (n-1). 

@IfMan
o(1,x) = var1{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{var1}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timstd
@Title     = Time standard deviation

@BeginDescription
Normalize by n. 

@IfMan
o(1,x) = std{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{std}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginOperator_timstd1
@Title     = Time standard deviation (n-1)

@BeginDescription
Normalize by (n-1). 

@IfMan
o(1,x) = std1{i(t',x), t_1<t'<=t_n}
@EndifMan
@IfDoc
@BeginMath
o(1,x) = \mbox{\textbf{std1}}\{i(t',x), t_1 < t' \leq t_n\}
@EndMath
@EndifDoc
@EndDescription
@EndOperator


@BeginExample
To compute the mean over all input timesteps use:
@BeginVerbatim
   cdo timmean ifile ofile
@EndVerbatim
@EndExample
