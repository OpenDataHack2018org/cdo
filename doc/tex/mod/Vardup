@BeginModule
@Name      = Vardup
@Title     = Parameter duplication
@Section   = Miscellaneous
@Arguments = ifile ofile
@Operators = pardup parmul

@BeginDescription
Duplicates all parameter of a dataset.
@EndDescription
@EndModule


@BeginOperator_pardup
@Title     = Duplicate parameter

@BeginDescription
Duplicates all parameters.
@EndDescription
@EndOperator


@BeginOperator_parmul
@Title     = Multiply parameter
@Parameter = nmul

@BeginDescription
Multiplies all parameters.
@EndDescription
@EndOperator

@BeginParameter
@Item = nmul
INTEGER  Number of multiplications
@EndParameter
